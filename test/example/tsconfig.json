{
  "compilerOptions":{
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib":[
      "es6",
    ],
    "sourceMap": true,
    "rootDir": ".",
    "noUnusedLocals": false,
    "plugins": []
  },
  "typeRoots": [
    "./node_modules/@types"
  ],
  "types": [
    "node",
    "mocha"
  ],
  "exclude":[
    "coverage",
    "node_modules",
    "!node_modules/@types"
  ]
}